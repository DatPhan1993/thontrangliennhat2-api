{
  "version": 2,
  "name": "thontrangliennhat-api",
  "builds": [
    {
      "src": "server-express.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server-express.js"
    },
    {
      "src": "/images/(.*)",
      "dest": "/server-express.js"
    },
    {
      "src": "/videos/(.*)",
      "dest": "/server-express.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/server-express.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server-express.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "server-express.js": {
      "maxDuration": 30
    }
  }
} 